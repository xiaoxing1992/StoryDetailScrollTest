<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SquareBView">
        <attr name="sq_view_height" format="dimension|integer" />
        <attr name="sq_right_height" format="dimension|integer" />
        <attr name="sq_left_radius" format="dimension|integer" />
        <attr name="sq_right_radius" format="dimension|integer" />
    </declare-styleable>


    <declare-styleable name="SquareBottomView">
        <attr name="sb_view_height" format="dimension|integer" />
        <attr name="sb_tilt_height" format="dimension|integer" />
        <attr name="sb_left_radius_height" format="dimension|integer" />
        <attr name="sb_right_radius_height" format="dimension|integer" />
    </declare-styleable>

    <declare-styleable name="IndicatorView">
        <!--        原点半径-->
        <attr name="ri_radius" format="dimension" />
        <!--        两个之间的间隔-->
        <attr name="ri_interval" format="dimension" />
        <!--        选中的颜色-->
        <attr name="ri_select_color" format="color|reference" />
        <!--        未选中的颜色-->
        <attr name="ri_unselect_color" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="PercentScrollBar">
        <attr name="ps_progress_aspect_ratio" format="float" />
        <attr name="ps_progress_color" format="color" />
        <attr name="ps_background_color" format="color" />
        <attr name="ps_radius" format="dimension" />
    </declare-styleable>
</resources>